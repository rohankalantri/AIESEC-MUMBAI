  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for AIESEC MUMBAI

	pod 'Firebase/Core'
	pod 'Firebase/Storage'
	pod 'Firebase/Database'
	pod ‘Firebase/RemoteConfig’
	pod ‘Firebase/Auth’
	pod ‘FirebaseUI’
	pod 'Eureka'
	pod ‘RealmSwift’
	pod ‘Alamofire’
	pod ‘SwiftOverlays’

  target 'AIESEC MUMBAITests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'AIESEC MUMBAIUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
